<%= simple_form_for (@painting) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :artist %>
  <%= f.input :year %>
  <%= f.input :price %>
  <%= f.input :style, collection: ["Nature", "Abstract", "Nude", "Impressionism", "Cubism"] %>
  <%= f.input :available %>
  <%= f.button :submit, 'Add photos', class: "btn btn-primary" %>
<% end %>
